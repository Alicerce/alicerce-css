{
	"blocks": "always",
	"brackets": "never",
	"colons": "never",
	"colors": "always",
	"commaSpace": "always",
	"commentSpace": "always",
	"cssLiteral": "never",
	"depthLimit": 2,
	"duplicates": true,
	"extendPref": "@extends",
	"indentPref": 2,
	"leadingZero": "never",
	"maxErrors": 0,
	"maxWarnings": 0,
	"mixed": true,
	"mixins": [],
	"namingConvention": "lowercase_underscore",
	"namingConventionStrict": true,
	"noImportant": true,
	"none": "never",
	"parenSpace": "never",
	"placeholders": "always",
	"prefixVarsWithDollar": true,
	"quotePref": "single",
	"semicolons": "never",
	"sortOrder": "alphabetical",
	"universal": "never",
	"valid": true,
	"zeroUnits": "never",
	"zIndexNormalize": 5,
  "efficient": "always",
  "exclude": [],
  "globalDupe": true,
  "groupOutputByFile" : true,
  "stackedProperties": "never",
  "trailingWhitespace": "never",
  "reporterOptions": {
		"columns": ["lineData", "severity", "rule", "description"],
		"columnSplitter": "  ",
		"showHeaders": false,
		"truncate": true
	}
}
